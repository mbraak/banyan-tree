{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "rules": {
        "brace-style": 0,
        "camelcase": 0,
        "comma-dangle": 0,
        "default-case": 0,
        "indent": 0,
        "max-len": 0,
        "new-cap": 0,
        "newline-per-chained-call": 0,
        "no-console": 0,
        "no-else-return": 0,
        "no-use-before-define": 0,
        "one-var": 0,
        "one-var-declaration-per-line": 0,
        "quotes": [2, "double", "avoid-escape"],
        "react/jsx-boolean-value": 0,
        "react/jsx-no-bind": 0,
        "react/no-multi-comp": 0,
        "react/sort-comp": 0
    }
}
