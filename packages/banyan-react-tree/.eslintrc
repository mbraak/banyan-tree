{
  "parser": "babel-eslint",
  "rules": {
    "camelcase": 0,
    "new-cap": 0,
    "no-console": 0,
    "no-unused-vars": 0,
    "no-use-before-define": 0,
    "strict": 0
  },
  "globals": {
    "console": false,
    "document": false,
    "localStorage": false,
    "module": false,
    "require": false,
    "setTimeout": false
  }
}
