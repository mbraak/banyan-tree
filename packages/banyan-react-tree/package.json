{
    "name": "banyan-react-tree",
    "version": "0.0.6",
    "description": "Banyan-react-tree is a tree component for react",
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/mbraak/banyan-react-tree.git"
    },
    "main": "lib/tree_component.js",
    "keywords": [
        "react",
        "react-component",
        "tree"
    ],
    "scripts": {
        "test": "jest",
        "lint": "tslint src/**/*.ts*",
        "test-watch": "jest --watch",
        "cover": "jest --coverage",
        "build": "webpack",
        "production": "rm -rf lib; tsc -p tsconfig.json; postcss -o banyan-react-tree.css css/*.css",
        "devserver": "webpack-dev-server"
    },
    "devDependencies": {
        "@types/cheerio": "^0.22.7",
        "@types/classnames": "^2.2.3",
        "@types/enzyme": "^3.1.8",
        "@types/jest": "^22.1.1",
        "@types/jsdom": "^11.0.4",
        "@types/lodash": "^4.14.99",
        "@types/react": "^16.0.36",
        "@types/react-dom": "^16.0.3",
        "cheerio": "^0.22.0",
        "css-loader": "^0.28.9",
        "dinosaur-data": "^1.0.0",
        "enzyme": "^3.3.0",
        "enzyme-adapter-react-16": "^1.1.1",
        "extract-text-webpack-plugin": "^3.0.2",
        "jest": "^22.1.4",
        "jsdom": "^11.6.2",
        "postcss": "^6.0.16",
        "postcss-cli": "^4.1.1",
        "postcss-load-config": "^1.2.0",
        "postcss-loader": "^2.0.10",
        "postcss-nested": "^3.0.0",
        "react": "^16.2.0",
        "react-dom": "^16.2.0",
        "react-test-renderer": "^16.2.0",
        "sinon": "^4.2.2",
        "style-loader": "^0.20.1",
        "ts-jest": "^22.0.2",
        "ts-loader": "^3.4.0",
        "tslint": "^5.9.1",
        "tslint-react": "^3.4.0",
        "typescript": "^2.7.1",
        "webpack": "^3.10.0",
        "webpack-dev-server": "^2.11.1",
        "webpack-node-externals": "^1.6.0"
    },
    "dependencies": {
        "banyan-immutable-tree": "^0.0.4",
        "classnames": "~2",
        "core-js": "~2",
        "fbjs": "~0.8"
    },
    "peerDependencies": {
        "react": "~16",
        "react-dom": "~16"
    },
    "jest": {
        "transform": {
            "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
        },
        "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "json"
        ]
    }
}
