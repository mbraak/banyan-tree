    "devDependencies": {
        "@types/cheerio": "^0.22.9",
        "@types/classnames": "^2.2.6",
        "@types/enzyme": "^3.1.13",
        "@types/enzyme-adapter-react-16": "^1.0.3",
        "@types/jest": "^23.3.2",
        "@types/jsdom": "^11.0.6",
        "@types/lodash": "^4.14.116",
        "@types/react": "^16.4.11",
        "@types/react-dom": "^16.0.7",
        "cheerio": "^0.22.0",
        "css-loader": "^1.0.0",
        "dinosaur-data": "^1.0.0",
        "enzyme": "^3.6.0",
        "enzyme-adapter-react-16": "^1.5.0",
        "extract-text-webpack-plugin": "v4.0.0-beta.0",
        "jest": "^23.5.0",
        "jsdom": "^12.0.0",
        "postcss": "^7.0.2",
        "postcss-cli": "^6.0.0",
        "postcss-load-config": "^2.0.0",
        "postcss-loader": "^3.0.0",
        "postcss-nested": "^4.0.0",
        "react": "^16.5.0",
        "react-dom": "^16.5.0",
        "react-test-renderer": "^16.5.0",
        "style-loader": "^0.23.0",
        "ts-jest": "^23.1.4",
        "ts-loader": "^5.0.0",
        "tslint": "^5.11.0",
        "tslint-react": "^3.6.0",
        "typescript": "^3.0.3",
        "webpack": "^4.17.2",
        "webpack-cli": "^3.1.0",
        "webpack-dev-server": "^3.1.8",
        "webpack-node-externals": "^1.7.2"
    },
    "dependencies": {
        "banyan-immutable-tree": "^0.0.5",
        "classnames": "~2",
        "core-js": "~2",
        "fbjs": "~0.8"
    },
    "peerDependencies": {
        "react": "~16",
        "react-dom": "~16"
    },
    "jest": {
        "transform": {
            "^.+\\.tsx?$": "ts-jest"
        },
        "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "json"
        ]
    }
}
