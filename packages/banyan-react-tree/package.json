{
    "name": "banyan-react-tree",
    "version": "0.0.7",
    "description": "Banyan-react-tree is a tree component for react",
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/mbraak/banyan-react-tree.git"
    },
    "main": "lib/tree_component.js",
    "keywords": ["react", "react-component", "tree"],
    "scripts": {
        "test": "jest",
        "lint": "tslint src/**/*.ts*",
        "test-watch": "jest --watch",
        "cover": "jest --coverage",
        "build": "webpack --mode development",
        "production":
            "rm -rf lib; tsc -p tsconfig.json; postcss -o banyan-react-tree.css css/*.css",
        "devserver": "webpack-dev-server --mode development"
    },
    "devDependencies": {
        "@types/cheerio": "^0.22.7",
        "@types/classnames": "^2.2.3",
        "@types/enzyme": "^3.1.10",
        "@types/jest": "^22.2.3",
        "@types/jsdom": "^11.0.4",
        "@types/lodash": "^4.14.107",
        "@types/react": "^16.3.12",
        "@types/react-dom": "^16.0.5",
        "cheerio": "^0.22.0",
        "css-loader": "^0.28.11",
        "dinosaur-data": "^1.0.0",
        "enzyme": "^3.3.0",
        "enzyme-adapter-react-16": "^1.1.1",
        "extract-text-webpack-plugin": "v4.0.0-beta.0",
        "jest": "^22.4.3",
        "jsdom": "^11.9.0",
        "postcss": "^6.0.21",
        "postcss-cli": "^5.0.0",
        "postcss-load-config": "^1.2.0",
        "postcss-loader": "^2.1.4",
        "postcss-nested": "^3.0.0",
        "react": "^16.3.2",
        "react-dom": "^16.3.2",
        "react-test-renderer": "^16.3.2",
        "sinon": "^4.5.0",
        "style-loader": "^0.21.0",
        "ts-jest": "^22.4.4",
        "ts-loader": "^4.2.0",
        "tslint": "^5.9.1",
        "tslint-react": "^3.5.1",
        "typescript": "^2.8.3",
        "webpack": "^4.6.0",
        "webpack-cli": "^2.0.14",
        "webpack-dev-server": "^3.1.3",
        "webpack-node-externals": "^1.7.2"
    },
    "dependencies": {
        "banyan-immutable-tree": "^0.0.5",
        "classnames": "~2",
        "core-js": "~2",
        "fbjs": "~0.8"
    },
    "peerDependencies": {
        "react": "~16",
        "react-dom": "~16"
    },
    "jest": {
        "transform": {
            "^.+\\.tsx?$": "ts-jest"
        },
        "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
        "moduleFileExtensions": ["ts", "tsx", "js", "json"]
    }
}
