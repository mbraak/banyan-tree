{
    "name": "banyan-immutable-tree",
    "version": "0.0.5",
    "license": "Apache-2.0",
    "keywords": [
        "tree"
    ],
    "scripts": {
        "test": "jest",
        "lint": "tslint src/**/*.ts*",
        "test-watch": "jest --watch",
        "cover": "jest --coverage",
        "build": "rm -rf lib; tsc -p tsconfig.json"
    },
    "devDependencies": {
        "@types/jest": "^23.3.2",
        "@types/lodash": "^4.14.116",
        "jest": "^23.6.0",
        "tree-model": "^1.0.7",
        "ts-jest": "^23.10.2",
        "tslint": "^5.11.0",
        "typescript": "^3.1.1"
    },
    "dependencies": {
        "immutable": "~3",
        "lodash": "~4"
    },
    "jest": {
        "transform": {
            "^.+\\.tsx?$": "ts-jest"
        },
        "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "json"
        ],
        "globals": {
            "ts-jest": {
                "diagnostics": false
            }
        }
    }
}
