{
    "name": "banyan-immutable-tree",
    "version": "0.0.4",
    "license": "Apache-2.0",
    "keywords": ["tree"],
    "scripts": {
        "test": "jest && tslint src/**/*.ts*",
        "test-once": "jest",
        "lint": "tslint src/**/*.ts*",
        "test-watch": "jest --watch",
        "cover": "jest --coverage",
        "production": "rm -rf lib; gulp"
    },
    "devDependencies": {
        "@types/jest": "^21.1.5",
        "@types/lodash": "^4.14.80",
        "babel-preset-env": "^1.6.1",
        "gulp": "^3.9.1",
        "gulp-babel": "^7.0.0",
        "gulp-sourcemaps": "^2.6.0",
        "gulp-typescript": "^3.2.3",
        "jest": "^21.2.1",
        "tree-model": "^1.0.6",
        "ts-jest": "^21.1.4",
        "ts-loader": "^3.1.1",
        "tslint": "^5.8.0",
        "typescript": "^2.6.1"
    },
    "dependencies": {
        "babel-core": "^6.26.0",
        "babel-polyfill": "^6.23.0",
        "core-js": "~2",
        "immutable": "^3.8.2",
        "lodash": "~4"
    },
    "jest": {
        "transform": {
            "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
        },
        "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
        "moduleFileExtensions": ["ts", "tsx", "js", "json"]
    }
}
