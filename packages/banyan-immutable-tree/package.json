{
    "name": "banyan-immutable-tree",
    "version": "0.0.4",
    "license": "Apache-2.0",
    "keywords": ["tree"],
    "scripts": {
        "test": "jest",
        "lint": "tslint src/**/*.ts*",
        "test-watch": "jest --watch",
        "cover": "jest --coverage",
        "build": "rm -rf lib; tsc -p tsconfig.json"
    },
    "devDependencies": {
        "@types/jest": "^22.2.3",
        "@types/lodash": "^4.14.107",
        "jest": "^22.4.3",
        "tree-model": "^1.0.6",
        "ts-jest": "^22.4.4",
        "tslint": "^5.9.1",
        "typescript": "^2.8.3"
    },
    "dependencies": {
        "immutable": "~3",
        "lodash": "~4"
    },
    "jest": {
        "transform": {
            "^.+\\.tsx?$": "ts-jest"
        },
        "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
        "moduleFileExtensions": ["ts", "tsx", "js", "json"]
    }
}
